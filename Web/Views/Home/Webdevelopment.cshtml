/* COHOSTWEB ANIMATION THEME */
/* ============================= */

:root {
  --cohost-primary: #2563eb;
  --cohost-secondary: #1e40af;
  --cohost-light: #eef2ff;
}

/* Smooth transition globally */
.sec-pad * {
  transition: all 0.4s ease;
}

/* Fade + slide animation on load */
.sec-pad {
  animation: pageFade 0.8s ease-in-out;
}

@@keyframes pageFade {
  from {
    opacity: 0;
    transform: translateY(20px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* Headings animation */
.sec-pad h2,
.sec-pad h3 {
  animation: fadeUp 0.9s ease both;
}

@@keyframes fadeUp {
  from {
    opacity: 0;
    transform: translateY(25px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* Paragraph animation */
.sec-pad p {
  animation: fadeText 1s ease both;
}

@@keyframes fadeText {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

/* Image animation */
.sec-pad img {
  animation: zoomIn 1s ease both;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.08);
}

@@keyframes zoomIn {
  from {
    opacity: 0;
    transform: scale(0.92);
  }
  to {
    opacity: 1;
    transform: scale(1);
  }
}

/* List animation */
.sec-pad ul li {
  animation: listFade 0.8s ease both;
}

@@keyframes listFade {
  from {
    opacity: 0;
    transform: translateX(-15px);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}

/* Section hover effect */
.sec-pad .mb-5:hover {
  transform: translateY(-3px);
}

/* CTA animation */
.sec-pad .bg-primary {
  animation: pulseGlow 2.8s infinite;
}

@@keyframes pulseGlow {
  0% {
    box-shadow: 0 0 0 0 rgba(37, 99, 235, 0.6);
  }
  70% {
    box-shadow: 0 0 0 20px rgba(37, 99, 235, 0);
  }
  100% {
    box-shadow: 0 0 0 0 rgba(37, 99, 235, 0);
  }
}

/* Hover effects */
.sec-pad img:hover {
  transform: scale(1.03);
}

.sec-pad h3:hover {
  color: var(--cohost-primary);
}

/* Mobile safe */
@@media (max-width: 768px) {
  .sec-pad img {
    margin-top: 20px;
  }
}

/* ========================= */
/* ATTRACTIVE BULLET HEADINGS */
/* ========================= */

.sec-pad .bullet-heading {
  display: flex;
  align-items: center;
  background: var(--cohost-light);
  border-left: 5px solid var(--cohost-primary);
  padding: 15px 20px;
  margin: 15px 0;
  border-radius: 8px;
  font-weight: 600;
  color: var(--cohost-secondary);
  transition: transform 0.3s ease, background 0.3s ease;
  cursor: default;
  animation: fadeUp 0.8s ease forwards;
}

.sec-pad .bullet-heading i {
  display: inline-block;
  margin-right: 12px;
  font-size: 18px;
  color: var(--cohost-primary);
}

/* Hover Effect */
.sec-pad .bullet-heading:hover {
  background: rgba(37, 99, 235, 0.1);
  transform: translateX(5px);
}

/* Reuse fadeUp animation */
@@keyframes fadeUp {
  from { opacity: 0; transform: translateY(20px);}
  to { opacity: 1; transform: translateY(0);}
}


