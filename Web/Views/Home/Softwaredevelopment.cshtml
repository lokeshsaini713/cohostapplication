@section Scripts {
        <script>
            document.addEventListener("DOMContentLoaded", function () {

                const metaTitle = "Custom Software Development Services | Cohostweb";
                const metaDescription = "Cohostweb offers custom software development services including web, mobile & enterprise solutions. Scalable, secure & business-ready software.";
                const metaKeywords = "";

                // TITLE
                document.title = metaTitle;

                // DESCRIPTION
                let descTag = document.querySelector('meta[name="description"]');
                if (!descTag) {
                    descTag = document.createElement('meta');
                    descTag.setAttribute('name', 'description');
                    document.head.appendChild(descTag);
                }
                descTag.setAttribute('content', metaDescription);

                // KEYWORDS (optional)
                if (metaKeywords) {
                    let keyTag = document.querySelector('meta[name="keywords"]');
                    if (!keyTag) {
                        keyTag = document.createElement('meta');
                        keyTag.setAttribute('name', 'keywords');
                        document.head.appendChild(keyTag);
                    }
                    keyTag.setAttribute('content', metaKeywords);
                }
            });
        </script>


}
  
 
  

  
  
   

  
   
  
